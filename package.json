{
  "name": "wongames",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@styled-icons/fa-brands": "^10.38.0",
    "@types/lodash.xor": "^4.5.7",
    "dotenv": "^16.0.1",
    "graphql": "^15.5.1",
    "joi": "^17.6.0",
    "lodash.xor": "^4.5.0",
    "next": "10.2.0",
    "next-auth": "^3.11.2",
    "next-pwa": "^5.2.21",
    "nextjs-progressbar": "^0.0.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.5",
    "styled-icons": "^10.38.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.4.1",
    "@types/jest": "^26.0.23",
    "@types/next-auth": "^3.7.0",
    "@types/node": "^18.6.4",
    "@types/react": "^17.0.19",
    "@types/react-slick": "^0.23.5",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.22.1",
    "apollo": "^2.33.10",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^10.5.4",
    "msw": "^0.44.2",
    "node-fetch": "^2.6.1",
    "plop": "^2.7.4",
    "prettier": "2.4.1",
    "typescript": "^4.2.4"
  }
}
